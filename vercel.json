{
  "build": {
    "env": {
      "DATABASE_URL": "@your-database-url",
      "NEXTAUTH_SECRET": "@your-nextauth-secret",
      "NEXT_PUBLIC_EXERCISEDB_API_KEY": "@your-exercisedb-api-key",
      "NEXT_PUBLIC_EXERCISEDB_HOST": "@your-exercisedb-host"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ]
}
